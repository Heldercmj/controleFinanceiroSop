<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee">

    <managed-bean>
        <managed-bean-name>entradaDadosBean</managed-bean-name>
        <managed-bean-class>com.com.beans.example.EntradaDadosBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <from-view-id>/entradaDados.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{entradaDadosBean.salvarDespesa}</from-action>
            <to-view-id>/consultaDespesas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
